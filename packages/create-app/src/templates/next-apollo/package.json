{
  "name": "next-apollo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "patch-package": "patch-package --exclude '^$'",
    "generate": "graphql-codegen && prisma generate",
    "dev": "graphql-codegen && prisma migrate dev && next dev",
    "build": "yarn -s generate && yarn -s lint --quiet && next build --no-lint",
    "start": "prisma migrate deploy && next start",
    "lint": "eslint src",
    "test": "exit 0",
    "postinstall": "yarn -s patch-package"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
