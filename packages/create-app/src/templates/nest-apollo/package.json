{
  "name": "nest-apollo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "gen": "prisma generate",
    "dev": "prisma migrate dev && NODE_ENV=development PORT=3000 nest start --watch",
    "build": "yarn gen && NODE_ENV=production nest build",
    "start": "prisma migrate deploy && NODE_ENV=production PORT=3000 node .nest/main.js",
    "lint": "yarn gen && eslint --fix src && tsc --incremental false",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@nestjs/apollo": "11.0.5",
    "@nestjs/cli": "9.4.2",
    "@nestjs/common": "9.4.0",
    "@nestjs/core": "9.4.0",
    "@nestjs/graphql": "11.0.5",
    "@nestjs/jwt": "10.0.3",
    "@nestjs/passport": "9.0.3",
    "@nestjs/platform-express": "9.4.0",
    "@nestjs/throttler": "4.0.0",
    "@prisma/client": "4.13.0",
    "apollo-server-express": "3.12.0",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "express": "4.18.2",
    "graphql": "16.6.0",
    "helmet": "6.1.5",
    "husky": "8.0.3",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "prisma": "4.13.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.0"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^18.12.1",
    "@types/bcryptjs": "^2",
    "@types/eslint": "^8",
    "@types/express": "^4",
    "@types/node": "^18.16.0",
    "@types/passport": "^1",
    "@types/passport-jwt": "^3.0.8",
    "@types/prettier": "^2",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "packageManager": "yarn@3.3.0"
}
