{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "builder": "swc",
    "assets": [
      {
        "outDir": "dist/public",
        "include": "../public"
      }
    ],
    "plugins": ["@nestjs/graphql"]
  }
}
