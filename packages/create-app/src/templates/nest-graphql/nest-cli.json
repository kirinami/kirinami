{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "builder": "swc",
    "assets": [
      {
        "outDir": "./.build/public",
        "include": "../public"
      }
    ],
    "plugins": [
      "@nestjs/graphql"
    ]
  }
}
