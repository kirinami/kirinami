{
  "name": "template",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=20.16"
  },
  "type": "module",
  "scripts": {
    "postinstall": "husky || true",
    "dev": "vite",
    "build:client": "vite build",
    "build:server": "vite build --ssr",
    "build": "node -e \"fs.promises.rm('.build', { recursive: true, force: true })\" && yarn build:client && yarn build:server",
    "start": "NODE_ENV=production node .build/server/index.js",
    "lint": "tsc --noEmit && eslint --fix src test index.ts",
    "test": "vitest run"
  },
  "dependencies": {
    "@fastify/static": "8.0.3",
    "@fastify/type-provider-json-schema-to-ts": "4.0.1",
    "@tanstack/react-query": "5.62.7",
    "clsx": "2.1.1",
    "dayjs": "1.11.13",
    "fastify": "5.2.0",
    "husky": "9.1.7",
    "i18next": "24.1.0",
    "immer": "10.1.1",
    "json-schema-to-ts": "3.1.1",
    "mime": "4.0.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "15.2.0",
    "react-router-dom": "7.0.2",
    "zustand": "5.0.2"
  },
  "devDependencies": {
    "@types/express": "5",
    "@types/mime": "4",
    "@types/node": "22.10.2",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.2",
    "@types/react-test-renderer": "19",
    "@vitejs/plugin-react-swc": "3.7.2",
    "acorn": "8.14.0",
    "autoprefixer": "10.4.20",
    "estree-walker": "3.0.3",
    "happy-dom": "15.11.7",
    "postcss": "8.4.49",
    "prettier": "3.4.2",
    "react-test-renderer": "19.0.0",
    "sass": "1.83.0",
    "tailwindcss": "3.4.16",
    "typescript": "5.7.2",
    "vite": "6.0.3",
    "vitest": "2.1.8"
  },
  "packageManager": "yarn@4.5.3"
}
